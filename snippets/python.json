	"pygame init": {
		"prefix": "pyg",
		"description": "Start a pygame project",
		"body": [
			"import pygame",
			"pygame.init()",
			"",
			"WIDTH, HEIGHT = $1, $2",
			"WIN  = pygame.display.set_mode((WIDTH, HEIGHT))",
			"pygame.display.set_caption('$4')",
			"",
			"FPS = $3",
			"",
			"def main():",
			"\trun = True",
			"\tclock = pygame.time.Clock()",
			"\twhile run:",
			"\t\tclock.tick(FPS)",
			"\t\tfor event in pygame.event.get():",
			"\t\t\tif event.type == pygame.QUIT:",
			"\t\t\t\tpygame.quit()",
			"\t\t\t\trun = False",
			"",
			"\t\tpygame.display.update()",
			"",
			"if __name__ == '__main__':",
			"\tmain()"
		]
	},
	"pygame_gui": {
		"prefix": "pyggui",
		"body": [
			"import pygame",
			"from pygame import Rect",
			"import pygame_gui",
			"",
			"pygame.init()",
			"",
			"FPS = 60",
			"WIDTH, HEIGHT = 500, 500",
			"WIN  = pygame.display.set_mode((WIDTH, HEIGHT))",
			"",
			"pygame.display.set_caption('Gui')",
			"",
			"background = pygame.Surface((WIDTH,HEIGHT))",
			"background.fill(pygame.Color('#000000'))",
			"",
			"manager = pygame_gui.UIManager((WIDTH, HEIGHT))",
			"",
			"hello_button = pygame_gui.elements.UIButton(relative_rect=pygame.Rect((350, 275), (100, 50)),text='Say Hello',manager=manager)",
			"def main():",
			"\trun = True",
			"\tclock = pygame.time.Clock()",
			"",
			"\twhile run:",
			"\t\ttime_delta = clock.tick(FPS)/1000.0",
			"",
			"\t\tfor event in pygame.event.get():",
			"\t\t\tif event.type == pygame.QUIT:",
			"\t\t\t\trun = False",
			"\t\t\tif event.type == pygame.USEREVENT:",
			"\t\t\t\tif event.user_type == pygame_gui.UI_BUTTON_PRESSED:",
			"\t\t\t\t\tif event.ui_element == hello_button:",
			"\t\t\t\t\t\tprint('Hello World!')",
			"",
			"",
			"\t\t\tmanager.process_events(event)",
			"\t\t",
			"\t\tmanager.update(time_delta)",
			"",
			"\t\tWIN.blit(background, (0,0))",
			"\t\tmanager.draw_ui(WIN)",
			"",
			"\t\tpygame.display.update()",
			"",
			"if __name__ == '__main__':",
			"\tmain()"
		]
	},
