{
	"configurations": {
		"foo": {},

		"node run": {
			"adapter": "vscode-node",
			"breakpoints": {
				"exception": {
					"all": "N",
					"uncaught": "Y"
				}
			},

			"configuration": {
				"request": "launch",
				"protocal": "auto",
				"stopOnEntry": true,
				"console": "integratedTerminal",
				"program": "${file}",
				"cwd": "${workspaceRoot}"
			}
		},

		"node attach": {
			"adapter": "vscode-node",
			"breakpoints": {
				"exception": {
					"all": "N",
					"uncaught": "Y"
				}
			},
			"configuration": {
				"name": "Attach to Process",
				"type": "node",
				"request": "attach",
				"skipFiles": ["<node_internals/**/*.js>"],
				"processId": "${processId}"
			}
		}


	}
}
